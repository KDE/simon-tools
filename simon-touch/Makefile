####### Install

install_itemfolder_01: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/opt/simontouch/qml/ || $(MKDIR) $(INSTALL_ROOT)/opt/simontouch/qml/ 
	-$(INSTALL_DIR) /home/claus/simon-tools-test/simon-touch/qml/simontouch $(INSTALL_ROOT)/opt/simontouch/qml/


uninstall_itemfolder_01:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/opt/simontouch/qml/simontouch
	-$(DEL_DIR) $(INSTALL_ROOT)/opt/simontouch/qml/ 


install_target: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/opt/simontouch/bin/ || $(MKDIR) $(INSTALL_ROOT)/opt/simontouch/bin/ 
	-$(INSTALL_PROGRAM) "$(QMAKE_TARGET)" "$(INSTALL_ROOT)/opt/simontouch/bin/$(QMAKE_TARGET)"
	-$(STRIP) "$(INSTALL_ROOT)/opt/simontouch/bin/$(QMAKE_TARGET)"

uninstall_target:  FORCE
	-$(DEL_FILE) "$(INSTALL_ROOT)/opt/simontouch/bin/$(QMAKE_TARGET)"
	-$(DEL_DIR) $(INSTALL_ROOT)/opt/simontouch/bin/ 


install:  install_itemfolder_01 install_target  FORCE

uninstall: uninstall_itemfolder_01 uninstall_target   FORCE

FORCE:

